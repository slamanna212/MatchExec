{
  "name": "match-bot",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "migrate": "npx tsx scripts/migrate.ts",
    "dev:all": "npm run migrate && npx pm2 start ecosystem.dev.config.js",
    "dev:stop": "npx pm2 stop ecosystem.dev.config.js",
    "dev:restart": "npx pm2 restart ecosystem.dev.config.js",
    "dev:logs": "npx pm2 logs",
    "build:processes": "npx tsc --noEmit --project tsconfig.json",
    "prod:start": "npm run migrate && npx pm2 start ecosystem.config.js",
    "prod:stop": "npx pm2 stop ecosystem.config.js",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "dependencies": {
    "@discordjs/voice": "0.19.0",
    "@mantine/core": "8.3.1",
    "@snazzah/davey": "^0.1.6",
    "@mantine/dates": "8.3.1",
    "@mantine/form": "8.3.1",
    "@mantine/hooks": "8.3.1",
    "@mantine/modals": "8.3.1",
    "@mantine/notifications": "8.3.1",
    "@tabler/icons-react": "3.34.1",
    "@types/express": "5.0.3",
    "@types/node-cron": "3.0.11",
    "bufferutil": "4.0.9",
    "discord.js": "14.22.1",
    "express": "5.1.0",
    "ffmpeg-static": "5.2.0",
    "framer-motion": "12.23.12",
    "lucide-react": "0.542.0",
    "next": "15.5.0",
    "node-cron": "4.2.1",
    "node-fetch": "2.7.0",
    "pm2": "6.0.11",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "sqlite3": "5.1.7",
    "tsx": "4.20.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@tailwindcss/postcss": "4",
    "@types/node": "24",
    "@types/react": "19",
    "@types/react-dom": "19",
    "eslint": "9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "4",
    "typescript": "5"
  }
}
